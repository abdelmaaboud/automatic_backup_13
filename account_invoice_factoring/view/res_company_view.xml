<?xml version="1.0" encoding="UTF-8"?>
<odoo>
	<data>
		<record model="ir.ui.view" id="view_company_form_factoring">
			<field name="name">view_company_form_factoring</field>
			<field name="model">res.company</field>
			<field name="inherit_id" ref="base.view_company_form"/>
			<field name="arch" type="xml">
                <xpath expr="(//notebook)" position="inside">
                    <page name="factoring" string="Factoring">
                        <field name="factoring_text" />
                    </page>
                </xpath>
			</field>
		</record>
	</data>
</odoo>
