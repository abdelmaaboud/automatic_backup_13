<odoo>
    <t t-name="barcode_scanner.incomming_process">
        <ol class="breadcrumb">
            <li id="home_breadcrumb"><a href="javascript:">Home</a></li>
            <li id="products_breadcrumb"><a href="javacript:">Products</a></li>
            <li id="product_breadcrumb"><a href="javascript:"><t t-esc="widget.product_key"/></a></li>
            <li class="active">Incomming</li>
        </ol>
        <t t-call="barcode_scanner.incomming.stepper"></t>
        <div id="barcode_scanner.incomming.app"/>
    </t>

    <t t-name="barcode_scanner.incomming.stepper">
        <div class="container mb32">
            <h2 class="text-center">Incomming Process</h2>
            <div class="row line">
                <div class="col-xs-3 stepText1">Step 1</div>
                <div class="col-xs-3 stepText2">Step 2</div>
                <div class="col-xs-3 stepText3">Step 3</div>
                <div class="col-xs-3 stepText4">Step 4</div>
            </div>
            <div class="row">
                <div class="col-xs-3"><div class="step step1"></div></div>
                <div class="col-xs-3"><div class="step step2"></div></div>
                <div class="col-xs-3"><div class="step step3"></div></div>
                <div class="col-xs-3"><div class="step step4"></div></div>  
            </div> 
        </div>
    </t>
</odoo>